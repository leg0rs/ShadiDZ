{
  "name": "@legors/nextjs-frontend-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "web:dev": "turbo run dev --filter=@legors/frontend",
    "web:build": "turbo run build --filter=@legors/frontend",
    "web:fix": "turbo run fix --filter=@legors/frontend",
    "build": "turbo run build --continue",
    "lint": "turbo run lint lint:ancillary --continue",
    "lint:ancillary": "prettier --check .github *.*",
    "test": "turbo run test --continue",
    "fix": "turbo run fix fix:ancillary --continue",
    "fix:frontend": "turbo run fix --continue --parallel",
    "fix:ancillary": "prettier --write .github *.*",
    "ci": "turbo run lint test --continue"
  },
  "packageManager": "pnpm@10.18.3",
  "devDependencies": {
    "@legors/tooling-config": "workspace:*",
    "eslint": "^9.39.1",
    "prettier": "3.6.2",
    "turbo": "^2.6.1"
  }
}